<?xml version='1.0' encoding='utf-8'?>
<widget id="com.stateshifterlabs.testapp" ios-CFBundleIdentifier="com.stateshifterlabs.testapp-app" osx-CFBundleIdentifier="com.stateshifterlabs.testapp-mac" version="1.0.11" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>test app</name>
    <description>
		just a test thingy
	</description>
    <author email="hello@stateshifterlabs.com">
		Stateshifter Labs
	</author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="phonegap-version" value="cli-6.1.0" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect --ignore-gpu-blacklist --enable-precise-memory-info" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <preference name="xwalkVersion" value="xwalk_core_library:18+" />
    <preference name="EnableViewportScale" value="true" />
    <preference name="BackupWebStorage" value="none" />
    <feature name="http://api.phonegap.com/1.0/notification" />
    <feature name="http://api.phonegap.com/1.0/geolocation" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#585858" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <platform name="ios">
    </platform>
    <platform name="osx">
        <preference name="WindowSize" value="auto" />
        <preference name="ShowTitle" value="true" />
    </platform>
    <gap:config-file parent="UIRequiresFullScreen" platform="ios">
        <false />
    </gap:config-file>
    <config-file method="replace" parent="LSApplicationCategoryType" platform="osx">
        <string>public.app-category.productivity</string>
    </config-file>
    <gap:config-file parent="UIStatusBarHidden" platform="ios">
        <true />
    </gap:config-file>
    <gap:config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <false />
    </gap:config-file>
    <plugin name="cordova-plugin-statusbar" spec="~2.2.3" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-console" spec="~1.0.7" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-sqlite-ext" spec="~0.10.4" />
    <engine name="browser" spec="~4.1.0" />
    <engine name="osx" spec="~4.0.1" />
    <engine name="ios" spec="~4.1.1" />
</widget>
